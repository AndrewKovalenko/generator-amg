{
  "debug": {
    "files": [
      {
      "expand": true,
      "cwd": "sources",
      "src": ["**"],
      "dest": "<%= deploymentDirectory %>/web"
    }
    ]
  },
  "release": {
    "files": [
      {
      "expand": true,
      "cwd": "sources",
      "src": ["index.html", 
        "js/libs/vendors/requirejs/require.js", 
        "js/libs/vendors/angular/angular",
        "js/libs/vendors/angular-ui-router/release/angular-ui-router",
        "views/**"
      ],
      "dest": "<%= deploymentDirectory %>/web"
  }]
  }
}
