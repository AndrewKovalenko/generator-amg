(function (require, undefined) {
  'use strict';

  require.config({
    baseUrl: 'js',

    paths:{
      'angular': 'libs/vendors/angular/angular',
      'angularUiRouter': 'libs/vendors/angular-ui-router/release/angular-ui-router'
    },

    shim:{
      'angular':{
        exports: 'angular'
      },
      'angularUiRouter': {
        deps:['angular'],
        exports: 'angularUiRouter'
      }
    }
  });

  var applicationStartModule = function(angular, routingConfiguration) {
    angular.element(document).ready(function() {
      var <%= applicationName %> = angular.module("<%= applicationName %>", 
                                               ["ui.router",
                                                 "app.controllers",
                                                 "app.directives",
                                                 "app.factories",
                                                 "app.services",
                                                 "app.filters"
                                               ]);
                                               <%= applicationName %>.config(routingConfiguration);
                                               angular.bootstrap(document, ['<%= applicationName %>']);

    });
  };

  require(['angular',
          'config/routing-configuration',
          'controllers-module',
          'directives-module',
          'factories-module',
          'services-module',
          'filters-module',
          'angularUiRouter'],
          applicationStartModule );
})(window.require);
