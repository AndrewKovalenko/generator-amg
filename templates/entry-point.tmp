(function (require) {
  'use strict';

  var applicationEntryPoint = function(requireConfiguration) {
    requireConfiguration.initialize();

    require([
      'angular',
      '<%= applicationName %>/config/routing-configuration',
      '<%= applicationName %>/controllers-module',
      '<%= applicationName %>/directives-module',
      '<%= applicationName %>/factories-module',
      '<%= applicationName %>/services-module',
      '<%= applicationName %>/filters-module',
      'utilities/sandbox',
      'angularUiRouter'
    ],
    function(angular, routingConfiguration) {
      angular.element(document).ready(function() {
        var entryPointModule = angular.module('<%= applicationName %>', 
                                              ['ui.router',
                                                'sandbox',
                                                '<%= applicationName %>.controllers',
                                                '<%= applicationName %>.directives',
                                                '<%= applicationName %>.services',
                                                '<%= applicationName %>.filters',
                                                '<%= applicationName %>.factories'
                                              ]);
                                              entryPointModule.config(routingConfiguration);
                                              angular.bootstrap(document, ['<%= applicationName %>']);

      });
    });
  };

  require(['config/requirejs-configuration'], applicationEntryPoint);
})(window.require);
