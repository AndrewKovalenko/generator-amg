(function(define) {
  'use strict';
  //NOTE define aliase for common namespace to load path 
  //configuration using window.define (requirejs define)

  define(
    ['config/requirejs-path'], 
    function(pathAliases) {
      return{
        initialize: function() {

          var requirejsModulesPath = {
            //Put your path configuration here
          };

          for(var alias in requirejsModulesPath){
            if(pathAliases.hasOwnProperty(alias)){
              throw new Error('Path configuration conflict');
            }

            pathAliases[alias] = requirejsModulesPath[alias];
          } 

          require.config({
            baseUrl: 'js',

            paths: pathAliases,

            shim:{
              'angular':{
                exports: 'angular'
              },
              'angularUiRouter': {
                deps:['angular'],
                exports: 'angularUiRouter'
              }
            }
          });
        }
      };
    });
})(window.define, window.angular);
